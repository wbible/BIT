package swing;

public class PersonDTO {
	private String id, name, pwd, tel;
	
	public PersonDTO() {
		
	}

	public PersonDTO(String id, String name, String email, String tel) {
		this.id = id;
		this.name = name;
		this.pwd = email;
		this.tel = tel;
	}

	public String getId() {
		return id;
	}

	public String getName() {
		return name;
	}

	public String getEmail() {
		return pwd;
	}

	public String getTel() {
		return tel;
	}
	
	
}
